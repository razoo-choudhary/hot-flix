<body>

<% if(typeof showHeaderFooter !== "undefined" && showHeaderFooter === true){ %> <%- include("header/content") %> <% } %>

<%- /** * @main body */ body %>

<% if(typeof showHeaderFooter !== "undefined" && showHeaderFooter === true){ %> <%- include("footer/content") %> <% } %>

<!-- JS -->
<script src="/resources/assets/javascript/khalti.min.js"></script>
<script src="/resources/assets/javascript/axios.min.js"></script>
<script src="/resources/assets/javascript/snackbar.min.js"></script>
<script src="/resources/assets/javascript/jquery-3.5.1.min.js"></script>
<script src="/resources/assets/javascript/bootstrap.bundle.min.js"></script>
<script src="/resources/assets/javascript/owl.carousel.min.js"></script>
