<script src="/resources/assets/javascript/photoswipe.min.js"></script>
<script src="/resources/assets/javascript/photoswipe-ui-default.min.js"></script>
<script>
    let config = {
        "publicKey": "<% if(typeof khalti_public_key !== "undefined" ){ %><%- khalti_public_key %><% } %>",
        "productIdentity": "premium-hot-flix",
        "productName": "premium hot flix",
        "productUrl": "http://localhost:5000",
        "paymentPreference": [
            "KHALTI",
            "EBANKING",
            "MOBILE_BANKING",
            "CONNECT_IPS",
            "SCT",
        ],
        "eventHandler": {
            onSuccess (payload) {
                axios.post("/verify-payment", payload).then(r => window.location.replace(r.data.location))
            }
        }
    };
</script>
<script src="/resources/assets/javascript/requests.js"></script>
<script src="/resources/assets/javascript/main.js"></script>
<script src="/resources/assets/javascript/handler.js"></script>
