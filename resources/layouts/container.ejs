<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- CSS -->
    <link rel="stylesheet" href="/resources/assets/stylesheets/bootstrap-reboot.min.css">
    <link rel="stylesheet" href="/resources/assets/stylesheets/bootstrap-grid.min.css">
    <link rel="stylesheet" href="/resources/assets/stylesheets/owl.carousel.min.css">
